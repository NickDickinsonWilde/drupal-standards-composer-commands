{
    "name": "nickwilde1990/drupal-standards-composer-commands",
    "description": "Provides composer commands for standards checks on your Drupal project",
    "type": "composer-plugin",
    "require": {
        "squizlabs/php_codesniffer": "^3.3",
        "drupal/coder": "^8.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "composer-plugin-api": "^1.1",
        "mouf/nodejs-installer": "^1.0"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Nick Wilde",
            "email": "nick@briarmoon.ca"
        }
    ],
    "autoload": {
        "psr-4": {
            "NickWilde1990\\DrupalStandardsCommands\\": "src/"
        }
    },
    "extra": {
        "class": "NickWilde1990\\DrupalStandardsCommands\\ComposerPlugin"
    }
}
